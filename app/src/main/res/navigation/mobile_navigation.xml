<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_history">

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.example.halojatahalloo.ui.history.HistoryFragment"
        android:label="@string/title_history"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.example.halojatahalloo.ui.contacts.ContactsFragment"
        android:label="@string/title_contacts" />

    <fragment
        android:id="@+id/navigation_dial"
        android:name="com.example.halojatahalloo.ui.dial.DialFragment"
        android:label="@string/title_dial"
        tools:layout="@layout/fragment_dial" />
</navigation>